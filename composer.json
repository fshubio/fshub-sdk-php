{
  "name": "fshub/sdk",
  "description": "The official PHP SDK for FsHub.",
  "type": "library",
  "keywords": [
    "fshub",
    "fshub.io",
    "lrm",
    "flightsim",
    "msfs2020",
    "xplane",
    "fsx",
    "ICAO"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Bobby Allen",
      "email": "ballen@bobbyallen.me",
      "homepage": "http://bobbyallen.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ballen/collection": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.4",
    "phpmd/phpmd": "^2.11",
    "squizlabs/php_codesniffer": "^3.6"
